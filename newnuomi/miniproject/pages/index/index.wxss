.userinfo{
  display: flex;
  flex-direction: column;
  align-items: center;
  /* background: url("../images/indexbanner.jpg") red no-repeat; */
  width: 750rpx;
  height: 300rpx;
  background-size: 750rpx 300rpx;
}
.userinfo image{
  width: 750rpx;
  height: 300rpx;
}
.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 11rpx auto;
  border-radius: 50%;
}

.userinfo-nickname{
  color: #fff;
}

.usermotto{
  margin-top: 200px;
}

.headborder{
	width: 150rpx;
	height: 150rpx;
	text-align: center;
	border: 1px solid #fd724a;
	border-radius: 100%;  
	margin:50rpx 20rpx 20rpx;
}

.usertask{
	width: 690rpx;
	margin: 0 auto;
}

.usertaskitems{
	display: flex;
	display: -webkit-flex;
	background: #fff;
	margin-bottom: 40rpx;
	width: 690rpx;
	height: 130rpx;
	border-radius: 8rpx;
	-webkit-box-shadow:0 0 12px #d8d8d8; 
	box-shadow:0 0 12px #d8d8d8;
	align-items:center;
	-webkit-align-items:center;
}

.rewardbg{
	color: #fff;
	width: 130rpx;
	height: 130rpx;
	background: url("data:image/png;base64,");
	background-size: 130rpx 130rpx;
	line-height: 130rpx;
	border-radius: 8rpx 0 0 8rpx;
	text-align: center;
	font-size: 35rpx;
	font-weight: bold;
}

.fuhao{
	font-size: 24rpx;
}

.tasktitle{
	width: 155rpx;
	text-align: center;
	font-size: 24rpx;
	color: #1a1e2a;
}

.progressbox{
	flex: 1;
	-webkit-flex:1;
}

.progressbox .txt1{
	font-size: 24rpx;
	color: #1a1e2a;
	line-height: 40rpx;
	text-align: center;
}

.progressbox .txt2{
	color: #ff0000;
	text-align: center;
}

.progressborder{
	position: relative;
	width: 100%;
	height: 20rpx;
	background: #e0e0e0;
	border-radius: 30rpx;
}

.progressbar{
	position: absolute;
	left: 0;
	top: 0;
	height: 20rpx;
	background: -webkit-linear-gradient(left, #f93c06 , #fe9c04); 
    background: linear-gradient(to right, #f93c06 , #fe9c04);
	border-radius: 30rpx;
}

.count-down{
	width: 160rpx;
	text-align: center;
	font-size: 24rpx;
	color: #1a1e2a;
}

.taskprogress{
	display: block;
	text-align: center;
	font-size: 24rpx;
	color: #7b7e83;
	line-height: 40rpx;
}

.receivebtn{
	padding: 5rpx 10rpx;
	border: 1px solid #f53224;
	border-radius: 5rpx;
	text-align: center;
	font-size: 24rpx;
	color:#f53224;
	width:80rpx;
	margin:0 auto;
	border-radius: 3px;
	height:36rpx;
}

.task-expired .rewardbg{
	background: #b9b9b9;
}

.task-expired .tasktitle,
.task-expired .progressbox .txt1,
.task-expired .progressbox .txt2,
.task-expired .count-down{
	color: #666;
}

.task-expired .progressbar{
	background: -webkit-linear-gradient(left, #555 , #b6b6b6); 
    background: linear-gradient(to right, #555 , #b6b6b6);
}

.task-award-title{
	position: relative;
	width: 690rpx;
	height: 95rpx;
	margin-top: 30rpx;
	text-align: center;
	color: #F23027;
	font-size: 40rpx;
}

.task-award-title image{
	position:absolute;
	top:15rpx;
	left:53rpx;
	height:40rpx;
}