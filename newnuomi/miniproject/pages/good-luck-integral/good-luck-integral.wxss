page{
  background: #fff !important;
}

.good-luck-banner {
  position: relative;
  overflow: hidden;
  width: 750rpx;
  height: 1100rpx;
}

.good-luck-banner .zhuanpan-bg {
  position: relative;
  width: 100%;
  height: 100%;
}

canvas {
  border: solid 0px black;
}

.zhuanpan-box {
  /* width: 650rpx;
  height: 650rpx; */
  position: absolute;
  top: 310rpx;
  margin: 0 auto;
  left: 0;
  right: 0;
}

.zhuanpan {
  width: 650rpx;
  height: 650rpx;
}

.btn-box {
  position: absolute;
  padding-top: 260rpx;
  top: 0;
  left: 0;
  right: 0;
  display: flex;
  -webkit-display: flex;
  justify-content: space-around;
  -webkit-justify-content: space-around;
}

.btn-box .rules-activity {
  /* flex: 1;
  -webkit-flex: 1; */
  color: #fff;
  text-align: center;
  /* margin-left: 60rpx; */
}

.btn-box .rules-activity image {
  width: 40rpx;
  height: 40rpx;
  /* margin-right: 10rpx; */
  vertical-align: -5rpx;
  margin-right: 10rpx;
}

.wan-yi-ci {
  width: auto;
  height: 60rpx;
  padding: 0 10rpx;
  line-height: 60rpx;
  text-align: center;
  color: #fff;
  font-size: 24rpx;
  background-color: #ff5736;
  border-radius: 2px;
  /* margin-left: 50rpx; */
}
.balance {
  height: 45rpx;
  line-height: 45rpx;
  text-align: center;
  color: #fff;
  font-size: 24rpx;
  border-radius: 2px;
  /* margin-left: 50rpx; */
}
.btn-box .rules-activity-btn {
  background: url("data:image/png;base64,") no-repeat;
  width: 290rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 30rpx;
  color: #fff;
  text-align: center;
  background-size: 290rpx 80rpx;
  margin-left: 57rpx;
}

.winning-list {
  position: absolute;
  clear: both;
  width: auto;
  height: auto;
  overflow: hidden;
  top: 240rpx;
  /*background: #fff;*/
  color: #fff;
}

.winning-list .winning-items{
  display: none;
}

.winning-title {
  text-align: center;
  font-size: 30rpx;
  line-height: 60rpx;
  color: #333;
  display: block;
  overflow: hidden;
}

.winning-title image {
  width: 80rpx;
  height: 80rpx;
}

.zhongjiang-text {
  float: left;
  margin-left: 230rpx;
  line-height: 80rpx;
  margin-right: 20rpx;
  color: #fff;
}

.renci-box {
  float: left;
  position: relative;
  width: 80rpx;
  height: 80rpx;
}

.number {
  position: absolute;
  text-align: center;
  width: 80rpx;
  line-height: 96rpx;
  color: #ddd;
  font-size: 36rpx;
  top: 0;
  left: 0;
}

.btn-box{
  top: 690rpx !important; 
}

.winning-items {
  display: flex;
  -webkit-display: flex;
  align-items: center;
  -webkit-align-items: center;
  border-bottom: 1px solid #eee;
  padding: 30rpx 30rpx 30rpx 20rpx;
}

.medal-box {
  width: 81rpx;
  height: 81rpx;
  margin-right: 10rpx;
  text-align: center;
  line-height: 81rpx;
  color: #333;
  font-size: 36rpx;
}

.medal-box .jinpai-img-box {
  width: 81rpx;
  height: 81rpx;
}

.winning-head {
  width: 95rpx;
  height: 95rpx;
  border-radius: 100%;
}

.winning-name {
  flex: 1;
  -webkit-flex: 1;
  font-size: 28rpx;
  line-height: 40rpx;
  padding-left: 20rpx;
  color: #666;
}

.winning-name .chouzhong {
  font-size: 30rpx;
  line-height: 50rpx;
}

.winning-name .chouzhong text {
  color: #ffab33;
}

.winning-time {
  color: #999;
  font-size: 26rpx;
}

.mask {
  position: fixed;
  background-color: #000;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 10;
  opacity: 0.5;
}

.chenggong-bg {
  position: fixed;
  z-index: 20;
  width: 450rpx;
  height: 453rpx;
  left: 50%;
  margin-left: -210rpx;
  top: 50%;
  margin-top: -226rpx;
}

.fail-bg-box {
  position: fixed;
  z-index: 20;
  width: 450rpx;
  height: 360rpx;
  left: 50%;
  margin-left: -210rpx;
  top: 50%;
  margin-top: -180rpx;
}

.fail-bg-box .fail-bg {
  width: 450rpx;
  height: 360rpx;
}

.gongxi-text {
  position: fixed;
  width: 450rpx;
  height: 450rpx;
  left: 50%;
  margin-left: -225rpx;
  top: 50%;
  margin-top: -226rpx;
  z-index: 20;
  color: #ffea3a;
  font-size: 30rpx;
  line-height: 55rpx;
  padding-top: 200rpx;
}

.gongxi-text text {
  display: block;
  text-align: center;
}

.chakan-jiangpin {
  width: 210rpx;
  height: 68rpx;
  line-height: 68rpx;
  text-align: center;
  background-color: #ffea3a;
  border-radius: 10rpx;
  margin: 30rpx auto;
  color: #f93b39;
  font-size: 30rpx;
}

.tap-block {
  position: absolute;
  width: 120rpx;
  height: 120rpx;
  background-color: transparent;
  z-index: 999999;
  margin: auto;
  left: 0;
  right: 0;
  top: 30rpx;
  bottom: 0;
}
